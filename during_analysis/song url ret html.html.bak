function saveSongName()
{
	var songname = $('#songname').val();
	var singername = $('#singername').val();
	if(songname == '' || songname == '你认为正确的') {
		alert('歌曲名称不能为空');
		return false;
	}
	if(singername == '') {
		alert('歌手名不能为空');
		return false;
	}
	var regrs = commonReg(songname);
	switch(regrs) {
		case 'link':
			alert('歌名中不能存在链接');
			return false;break;
		case 'stab':
			alert('歌名中不允许有自定义标签');
			return false;break;
		case 'tab':
			alert('歌名中不允许有标签');
			return false;break;
		default:
			break;
	}
	var regrs = commonReg(singername);
	switch(regrs) {
		case 'link':
			alert('歌手名中不能存在链接');
			return false;break;
		case 'stab':
			alert('歌手名中不允许有自定义标签');
			return false;break;
		case 'tab':
			alert('歌手名中不允许有标签');
			return false;break;
		default:
			break;
	}
	
	var songid = 2245322;
	var operurl = "/info_oper.php?tag=addsongname&songid="+songid+"&songname="+songname+"&singername="+singername;
	$.ajax({
		type : "GET",
		url : operurl,
		contentType : "text/html;charset=gb2312",
		success : function(msg){
			if(msg == 'nologin'){
				alert('对不起，您需要登录才能添加歌曲名');
			}
			else if(msg == 'userStatError') {
				alert('您的帐户已经被冻结，不能做任何操作');
			}
			else if(msg == 'nosongname') {
				alert('歌曲名不能为空');
			}
			else if(msg == 'exceedlimit') {
				alert('添加的歌曲名不能超过3个');
			}
			else if(msg == 'userlimit') {
				alert('您只能为该歌曲添加一个歌曲名');
			}
			else if(msg == 'existed') {
				alert('您添加的歌名已存在');
			}
			else if(msg == 'getNameError') {
				alert('获取歌名列表失败，请刷新');
			}
			else{
				$('ul.has_hb').html(msg);
			}
		}
	});
}

function favSongName(nameid)
{
	var favsonglist = getCookie('favSongName');
	if(favsonglist == null){
		favsonglist = '';
	}
	if(favsonglist.length > 0 && favsonglist.indexOf(nameid)>-1) {
		alert('您已经顶过该歌曲名');
		return false;
	}
	var operurl = "/info_oper.php?tag=favsongname&nameid="+nameid;
	$.ajax({
		type : "GET",
		url : operurl,
		success : function(msg){
			if(msg == 'nologin'){
				alert('对不起，您需要登录才能添加歌曲名');
			}
			else if(msg == 'userStatError') {
				alert('您的帐户已经被冻结，不能做任何操作');
			}
			else{
				$('#favname_'+nameid).html('顶'+msg);
				if(favsonglist != '') {
					favsonglist += ',';
				}
				favsonglist += nameid;
				setCookie('favSongName', favsonglist);
			}
		}
	});
}
