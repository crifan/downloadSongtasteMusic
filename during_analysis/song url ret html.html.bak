<script type="text/javascript">
//EvPNG.fix('div, ul, img, li, input');
function setCookie(name, value)
{
	var Days = 90;
	var exp  = new Date();
	exp.setTime(exp.getTime() + Days*24*60*60*1000);
	document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();

}

function getNowStamp()
{
	var timestamp = Date.parse(new Date());
	return parseInt(timestamp/1000);
}

function commonReg(cont)
{
		if(cont.length < 1){
			return 'blank';
		}
		var pattern = /\.com|\.cn|\.net|\.org|\.biz|\.info|\.cn|\.mobi|\.name|\.sh|\.ac|\.la|\.travel|\.tm|\.us|\.cc |\.tv| \.mn| \.saia| \.hn|\.lc |\.hk|\.bz|\.ws |\.tel|\.cm|\.tw|\.io|\.ac/gi;
		var stpattern = /songtaste.com/i;
		if (!stpattern.test(cont)) {
			if (pattern.test(cont)) {
				return 'link';
			}
		}

		var par = /\[.*?\].*?\[\/.*?\]*?]/i;
		if (par.test(cont)) {
			return 'stab';
		}

		var reg = new RegExp('[^<>]*?>|Uis');
	  	if (reg.test(cont)) {
			return 'tab';
	  	}
		return '';
}

function createcode() {
	var ih = $("#secoder_img:has(img)");
	if(ih.length == 0) {
		$("#secoder_img").append("<img src='"+ createimg() + "' title='输入不区分大小写' /><span style='font-size:10px;'><a href='javascript:;' onclick=\"$('#secoder_img img').attr('src',function(){return createimg();})\">看不清楚？换一张</a></span>");
	}
}
function createimg() {
	var img = "/Secoder.php?" + codehash(7);
	return img;
}
function check_code() {
	var code = $('#secoder').val();
	var re = $.ajax({
	                  url: "/info_oper.php?tag=check_code",
	                  data: "coder="+code,
	                  type: 'POST',
	                  async: false
	             }).responseText;
	if(re == 'error') return false;
	return true;
}

